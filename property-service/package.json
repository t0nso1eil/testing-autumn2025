{
  "name": "property-service",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPatterns=\"integration.test\" --forceExit --detectOpenHandles",
    "test:integration:property": "jest --testPathPatterns=\"property.integration.test\" --forceExit --detectOpenHandles",
    "test:unit": "jest --testPathPatterns=\"test.ts\" --forceExit --detectOpenHandles",
    "test:all": "jest --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "testcontainers": "^11.9.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
